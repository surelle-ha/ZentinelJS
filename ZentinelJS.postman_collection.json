							"script": {
								"exec": [
									"var responseJson = pm.response.json();\r",
									"\r",
									"if (responseJson.status === \"success\" && responseJson.userData && responseJson.userData.token) {\r",
									"    var token = responseJson.userData.token;\r",
									"    pm.environment.set(\"AUTH_TOKEN\", token);\r",
									"    console.log(\"Token saved as environment variable 'authToken'.\");\r",
									"} else {\r",
									"    console.error(\"Failed to retrieve the token. Response:\", responseJson);\r",
									"}\r",
